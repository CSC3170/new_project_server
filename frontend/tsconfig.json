{
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
